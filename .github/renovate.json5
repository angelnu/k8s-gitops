{
  extends: ["github>angelnu/renovate-config:default-automerge"],
  platform: "github",
  username: "angelnu-bot[bot]",
  gitAuthor: "angelnu-bot[bot] <115925344+angelnu-bot[bot]@users.noreply.github.com>",
  repositories: ["angelnu/k8s-gitops"],
  flux: {
    managerFilePatterns: [
      "/(apps|base|clusters|core|crds|settings)/.+\\.ya?ml$/",
    ],
  },
  "helm-values": {
    managerFilePatterns: [
      "/(apps|base|clusters|core|crds|settings)/.+\\.ya?ml$/",
    ],
  },
  kubernetes: {
    managerFilePatterns: [
      "/(apps|base|clusters|core|crds|settings)/.+\\.ya?ml$/",
    ],
  },
  packageRules: [
    {
      matchPackageNames: [
        "registry.ci.openshift.org/origin/release-scos",
        "quay.io/okd/scos-release",
      ],
      versioning: "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-okd-scos\\.(?<build>\\d+)$",
      ignoreUnstable: true,
      allowedVersions: "/^\\d+\\.\\d+\\.\\d+-okd-scos\\.\\d+$/",
    },
  ],
}
